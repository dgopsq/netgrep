{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/core/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "make",
        "cwd": "packages/core",
        "outputPath": "packages/core/pkg"
      }
    },
    "test": {
      "executor": "@nxrs/cargo:test",
      "options": {}
    },
    "lint": {
      "executor": "@nxrs/cargo:clippy",
      "options": {
        "fix": false,
        "failOnWarnings": true,
        "noDeps": true
      }
    }
  },
  "tags": []
}
